{
  "id": "blog_post",
  "label": "Blog Post",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "uid": {
        "type": "UID",
        "config": {
          "label": "Slug",
          "placeholder": ""
        }
      },
      "category": {
        "type": "Link",
        "config": {
          "label": "Category",
          "select": "document",
          "customtypes": [
            "category"
          ]
        }
      },
      "author": {
        "type": "Group",
        "config": {
          "label": "Authos",
          "fields": {
            "author": {
              "type": "Link",
              "config": {
                "label": "author",
                "select": "document",
                "customtypes": [
                  "author"
                ]
              }
            }
          }
        }
      },
      "date": {
        "type": "Date",
        "config": {
          "label": "date",
          "placeholder": ""
        }
      },
      "thumbnail": {
        "type": "Image",
        "config": {
          "label": "Thumbnail",
          "constraint": {},
          "thumbnails": []
        }
      },
      "summary": {
        "type": "StructuredText",
        "config": {
          "label": "Summary",
          "placeholder": "Write a summary in 3 sentences",
          "allowTargetBlank": true,
          "single": "paragraph,strong,em,hyperlink,rtl"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "content": {
              "type": "SharedSlice"
            },
            "code": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}